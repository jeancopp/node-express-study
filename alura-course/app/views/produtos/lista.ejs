<html>

<body>
    <% if (lista) { %>
    <table border="1px">
        <thead>
            <tr>
                <td>Código</td>
                <td>Nome</td>
                <td>Descrição</td>
                <td>Preço</td>
            </tr>
        </thead>
        <tbody>
            <% for(var i=0; i<lista.length; i++) {%>
                <tr>
                    <td> <%= lista[i].id %> </td>
                    <td> <%= lista[i].titulo %> </td>
                    <td> <%= lista[i].descricao %> </td>
                    <td> <%= lista[i].preco %> </td>
                </tr>
            <% } %>
        </tbody>
    </table>
    <% } else {%>
        <h3>Sem retorno do servidor</h3>
    <% } %>
</body>

</html>

